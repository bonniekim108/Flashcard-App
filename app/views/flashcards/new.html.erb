<%= form_for @flashcard do |f| %>
 <%= f.label :word %><%= f.text_field :word, value: "" %><br>
 <%= f.label :pronunciation %><%= f.text_field :pronunciation %><br>
 <%= f.label :definition %><%= f.text_field :definition %><br>
 <%= f.label :related_words %><%= f.text_field :related_words %><br>
 <%= f.label :notes %><%= f.text_area :notes %><br>
 <%= f.submit "Add to flashcard" %>
<% end %>

	<%= link_to "Show flashcards", flashcards_path %>
